{
    "name": "joaommota-next-website",
    "version": "0.1.0",
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./**/*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "scripts": {
